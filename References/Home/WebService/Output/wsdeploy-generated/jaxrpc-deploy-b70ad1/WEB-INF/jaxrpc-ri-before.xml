<?xml version="1.0" encoding="UTF-8"?>

<webServices
    xmlns="http://java.sun.com/xml/ns/jax-rpc/ri/dd"
    version="1.0"
    targetNamespaceBase="http://www.shoemake.com/wsdl"
    typeNamespaceBase="http://www.shoemake.com/types"
    urlPatternBase="/ws">

    <endpoint
        name="UserService"
        displayName="A User Manager Service"
        description="A Web Service to manage the list of users" 
        interface="mjs.home.commands.users.UserWebService"  
        implementation="mjs.home.commands.users.UserWebServiceImpl"
	    model="/WEB-INF/model.gz">
		</endpoint>
 
    <endpointMapping
        endpointName="UserService"
        urlPattern="/UserService"/>

</webServices>
